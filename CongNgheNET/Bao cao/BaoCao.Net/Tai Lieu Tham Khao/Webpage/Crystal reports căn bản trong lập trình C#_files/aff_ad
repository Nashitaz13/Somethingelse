    <script>
        function sendmobilenumber() {
            mobilenumber = $('#txtphone').val();
            var phoneno1 = /^\d{11}$/;
            var phoneno = /^\d{10}$/;
            if (mobilenumber.match(phoneno) || mobilenumber.match(phoneno1)) {
                $.getJSON(
                    'http://campaign.lazada.vn/_itool/sms/save?callback=?', {
                        'phone': $("#txtphone").val(),
                        'type': 'home'
                    },
                    function(data) {
                        if (data.error == 1) {
                            alert("Số điện thoại bạn nhập không đúng. Xin vui lòng thử lại.");
                        }
                        if (data.error == 2) {
                            alert("Số điện thoại của bạn đã tồn tại trong hệ thống Lazada.");
                        }
                        if (data.error == 0) {
                            alert("Cảm ơn bạn. Chúng tôi sẽ gởi thông tin qua số điện thoại của bạn.");
                            $("#txtphone").val($("#txtphone").data('placeholder'));
                        }
                    }
                );
            } else {
                alert("Vui lòng nhập số điện thoại di động của bạn!");
                document.getElementById('txtphone').focus();
                return false;
            }
        }
    </script>
