	var newpricebi = searchallBi(skubi);
	if(newpricebi != undefined)
	{	  
		$(this).prevAll("div").each(function(){
			if($(this).hasClass("product-card__sale")) {
				$(this).html("- 15%");
			} else if($(this).hasClass("product-price__saving")) {
				$(this).html("- 15%");
			}
		});

	}
});



