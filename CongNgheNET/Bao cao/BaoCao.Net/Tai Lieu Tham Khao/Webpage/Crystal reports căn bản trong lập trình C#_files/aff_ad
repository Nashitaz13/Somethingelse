$(document).ready(function(){
    GetBrandSKU("/mobapi/san-pham-chong-nang/?maxitems=8", ".brand-01 .sku-container");
    GetBrandSKU("/mobapi/san-pham-chong-mua/?maxitems=8", ".brand-02 .sku-container"); // when your brand cannot use /mobapi then use like this   
});

/*
GetBrandSKU("[Link of brand page]", "target class css to show kup sku");
*/
function GetBrandSKU(url, target) {
    $.get(url, function(o){ // call max item = 8
        // call back when ajax finished
        $(target).empty();
        $.each(o.metadata.results, function(k,v){
            if(k >= 8) return;
            var url = v.data.url;
            var img = (v.images.length > 0)? v.images[0].path : "";
            var title = v.data.name;
            var price = +v.data.price;
            var sprice = +v.data.special_price;
            var discount = v.data.max_saving_percentage;
            $(target).append(GetSkuHTML(url, img, title, price, sprice, discount));
        });
    });
}

/* format currency in THai
remove .00 and add (,)
*/

function formatCrrNumer(num) {var p = num.toFixed(2).split(".");return "" + p[0].split("").reverse().reduce(function(acc, num, i, orig) {return  num + (i && !(i % 3) ? "," : "") + acc;}, "");}
function GetSkuHTML(url, img, title, price, sprice, discount) { // use for generate sku's HTML

    url = url.replace("/mobapi","");

    var html = "";
        html += "<a class='dd' href='"+url+"'>";
        html += "<div class='img' style='background-image: url("+img+")'></div>";
        html += "<div class='title'>"+title+"</div>";

        if(isNaN(sprice) || sprice == 0) {
            html += "<div class='sprice nodc'>"+formatCrrNumer(price)+" VND</div>";
        } else {
            discount = (discount == null)? Math.ceil(100 - sprice*100/price) : discount;
            html += "<div class='sprice'>"+formatCrrNumer(sprice)+" VND</div>";
            html += "<div class='price'>"+formatCrrNumer(price)+" VND</div>";
            html += "<div class='discount'>"+ discount +"%</div>";
        }
        
        html += "</a>";
    return html;
}

$(function(){
    if( /iPad/i.test(navigator.userAgent) ) {
        $(".wrapper.sticky-nav .nav").css("right", "0");
    }
});

viewport = document.querySelector("meta[name=viewport]");
viewport.setAttribute('content', 'width=1200');

</script>
