            for(var i=0; i<ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0)==' ') c = c.substring(1);
                if (c.indexOf(name) != -1) return c.substring(name.length,c.length);
            }
            return "";
        }
        function setCookie(cname, cvalue, exhours) {
            var d = new Date();
            d.setTime(d.getTime() + (exhours*60*60*1000));
            var expires = "expires="+d.toGMTString();
            document.cookie = cname + "=" + cvalue + "; " + expires +"; path=/";
        }
        var sessionCounter = getCookie('session_counter');
        var utmaValue = getCookie('__utma');
        if (utmaValue != '') {
            var gaSessionCounter = parseInt(utmaValue.split('.').pop());
            if (gaSessionCounter != '' && sessionCounter != gaSessionCounter) {
                var url = "/tracking/sessioncounter/";
                var csrfTokenName = 'FORM_TOKEN';
                var csrfToken = '6158d347651a6c4a782cf259b792e8788de2ca0c';
                if (typeof(jQuery) != "undefined") {
                    jQuery.ajax({
                        url: url,
                        type: 'post',
                        dataType: 'json',
                        data: {
                            csrfTokenName: csrfTokenName,
                            csrfToken: csrfToken
                        }
                    });
                }
                else {
                    var http = new XMLHttpRequest();
                    var params = "csrfTokenName=" + csrfTokenName + "&csrfToken=" + csrfToken;
                    http.open("POST", url, true);
                    http.setRequestHeader("Content-type", "application/x-www-form-urlencoded; charset=UTF-8");
                    http.setRequestHeader("Content-length", params.length);
                    http.setRequestHeader("X-Requested-With", "XMLHttpRequest");
                    http.setRequestHeader("Connection", "close");
                    http.send(params);
                }
            }
            setCookie('session_counter', gaSessionCounter, 1);
        }
    }]);

    
    
            _gaq.push(['_trackPageview']);
    </script>
