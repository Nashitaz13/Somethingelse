<script>
$(document).ready(function(){

/* Installment */
$('.product-card .product-card__price').each(function() {
var lastPriceString = $(this).text().replace('VND','').replace(/\./g, '').trim();
var lastPriceInt = parseInt(lastPriceString);
	if (lastPriceInt >= 3000000) {
		var newPrice = lastPriceInt/12;	
                $(this).addClass("install-update");		
		$(this).append('<div class="new-price-installment"><span>Trả góp <span>0% </span>từ </span>' + (Math.floor(newPrice/100)*100).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' VND / tháng' + '</div>' );
	}
});

$('.product-list .price').each(function() {
var lastPriceString = $(this).text().replace('VND','').replace(/\./g, '').trim();
var lastPriceInt = parseInt(lastPriceString);
	if (lastPriceInt >= 3000000) {
		var newPrice = lastPriceInt/12;		
                $(this).addClass("install-update");	
		$(this).append('<div class="new-price-installment"><span>Trả góp <span>0% </span>từ </span>' + (Math.floor(newPrice/100)*100).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' VND / tháng' + '</div>' );
	}
});
$('.productsCatalog .product-item .product-price__current').each(function() {
$this = $(this);

var lastPriceString = $(this).text().replace('VND','').replace(/\./g, '').trim();
var lastPriceInt = parseInt(lastPriceString);
	if (lastPriceInt >= 3000000) {
		var newPrice = lastPriceInt/12;		
                $(this).addClass("install-update");	
		$(this).append('<div class="new-price-installment"><span>Trả góp <span>0% </span>từ </span>' + (Math.floor(newPrice/100)*100).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' VND / tháng' + '</div>' );
	}

});

/* End Installment */





/* Replace 15% */
$('.product-item.bi_ti .product-price div').each(function() {
    if($(this).hasClass("product-price__saving")) {
        $('.product-item.bi_ti .product-price__saving').html("- 15%")
    }
});

$('.product-card.bi_ti .price-block--grid div').each(function() {
    if($(this).hasClass("product-card__sale")) {
        $('.product-card.bi_ti .product-card__sale').html("- 15%")
    }
});
/* Replace 15% */

/* Discount 10% */
$('.productsCatalog li.fri_10 .itm-price').each(function() {
    $this = $(this);
    if ($this.hasClass('old')) {
        return;
    }
    else {
        var lastPriceString = $(this).text().replace('VND','').replace(/\./g, '').trim();
        var lastPriceInt = parseInt(lastPriceString);
        var newPrice;

        newPrice = lastPriceInt - lastPriceInt * 0.1;
        
        $(this).removeClass('install-update');
        $(this).find('.new-price-installment').remove();
        $(this).next('.new-price-all').remove();
        $(this).after('<div class="new-price-all new-price-10"><div class="np-left"><div class="np-left-title">Nhập mã</div><div class="np-voucher">FRISO1</div></div><div class="np-right">' + (Math.floor(newPrice/100)*100).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' VND' + '</div></div>' );
    }
});

$('.product-card.fri_10 .product-card__price').each(function() {
    var lastPriceString = $(this).text().replace('VND','').replace(/\./g, '').trim();
    var lastPriceInt = parseInt(lastPriceString);
    var newPrice;

    newPrice = lastPriceInt - lastPriceInt * 0.1;

    $(this).removeClass("install-update");
    $(this).find(".new-price-installment").remove();
    $(this).next(".new-price-all").remove();
    $(this).after('<div class="new-price-all new-price-10"><div class="np-left"><div class="np-left-title">Nhập mã</div><div class="np-voucher">FRISO1</div></div><div class="np-right">' + (Math.floor(newPrice/100)*100).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' VND' + '</div></div>' );
});
/* Discount 10% */



function searchallh(searchType){
	var DEFAULT_TEXT = {};
        DEFAULT_TEXT['PH846HBAA155GIVNAMZ-1671050'] = '1.287.000 VND';
DEFAULT_TEXT['PH846HBAA155GJVNAMZ-1671051'] = '777.000 VND';
DEFAULT_TEXT['PH846HBAA155GKVNAMZ-1671052'] = '2.059.000 VND';
DEFAULT_TEXT['PH846HBAA155GLVNAMZ-1671053'] = '1.617.000 VND';
DEFAULT_TEXT['PH846HBAA155GMVNAMZ-1671054'] = '1.032.000 VND';



  	return DEFAULT_TEXT[searchType]; 
}

$('.button-buy').each(function() {
	var sku = $(this).attr('data-sku-simple');
	var newpriceh = searchallh(sku);
	if(newpriceh == undefined)
	{
	  return;
	} else
	{

	$(this).siblings(".product-card__price").removeClass("install-update");
	$(this).siblings(".product-card__price").find(".new-price-installment").remove();
	$(this).siblings(".new-price-all").remove();
        $(this).after('<div class="new-price-all new-price-0"><div class="np-left"><div class="np-left-title">Nhập mã</div><div class="np-voucher">PHS150</div></div><div class="np-right">' + newpriceh + '</div></div>' );
	}
});


});

</script>


