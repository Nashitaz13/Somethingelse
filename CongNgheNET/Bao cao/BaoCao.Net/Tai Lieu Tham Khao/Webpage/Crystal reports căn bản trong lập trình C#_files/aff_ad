/* Flash sale catalog */
$('.button-buy').each(function() {
	var sku = $(this).attr('data-sku-simple');
	var newpricef = searchallF(sku);
	if(newpricef != undefined)
	{
	  
	$(this).siblings(".product-card__price").removeClass("install-update");
	$(this).siblings(".product-card__price").find(".new-price-installment").remove();
	$(this).siblings(".new-price-all").remove();
	$(this).after('<div class="new-price discount">' + newpricef + '</div>');
	}
});

/* Flash sale SIS */
$('.buy_btn').each(function() {
	var sku = $(this).attr('data-sku-simple');
	var newpricef = searchallF(sku);
	if(newpricef != undefined)
	{
	  
	$(this).closest(".product-item__inputs-wrapper").prev(".product-item__content").find(".product-price__current").removeClass("install-update");
	$(this).closest(".product-item__inputs-wrapper").prev(".product-item__content").find(".new-price-installment").remove();

	$(this).closest(".product-item__inputs-wrapper").prev(".product-item__content").find(".product-price__row:first-child").after('<div class="new-price discount" ' + 'style="margin-top: 2px;"' +'>' + newpricef + '</div>');
	}
});


