<script>
    $('.cate-menu li a').each(function() {
        $this = $(this);
        $this.closest('li').removeClass('active');
        var currentUrl = window.location.pathname;
        if ($this.attr('href').search(currentUrl) != -1) {
            $this.closest('li').addClass('active');
        }
    });
    $('.brand-menu a').each(function() {
        $this = $(this);
        $this.removeClass('active');
        var currentUrl = window.location.pathname;
        if (($this.attr('href').search(currentUrl) != -1) && currentUrl != "/tuan-le-thoi-trang-hang-hieu-gia-hoi/") {
            $this.addClass('active');
        }
    });
    $('.fp-menu li a').each(function() {
        $this = $(this);
        $this.closest('li').removeClass('active');
        var currentUrl = window.location.pathname;
        if ($this.attr('href').search(currentUrl) != -1) {
            $this.closest('li').addClass('active');
            $('.fp-anchor').addClass('active');
        }
    });
	$('.top-menu-fa li a').each(function() {
        $this = $(this);
        $this.closest('li').removeClass('active');
        var currentUrl = window.location.pathname;
        if ($this.attr('href').search(currentUrl) != -1) {
            $this.closest('li').addClass('active');
        }
    });
    $('.top-menu li a').each(function() {
        $this = $(this);
        $this.closest('li').removeClass('active');
        var currentUrl = window.location.pathname;
        if ($this.attr('href').search(currentUrl) != -1) {
            $this.closest('li').addClass('active');
            $('.li').addClass('active');
        }
    });
    $('.laz-first-nav li a').each(function() {
        $this = $(this);
        $this.closest('li').removeClass('active');
        var currentUrl = window.location.pathname;
        if ($this.attr('href').search(currentUrl) != -1) {
            $this.closest('li').addClass('active');
            $('.li').addClass('active');
        }
    });
</script>

