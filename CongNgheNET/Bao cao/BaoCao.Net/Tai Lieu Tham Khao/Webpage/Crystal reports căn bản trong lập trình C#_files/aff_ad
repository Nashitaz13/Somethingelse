$(".stackbanner").css({"display":"none"});
$(window).scroll(function(){
if ( $(window).scrollTop() > $(window).height()/1.6 && ($(window).scrollTop()+ $(window).height()) <= $(".footer_new-2015").offset().top){
$(".stackbanner").css({"display":"block"});
} else{
$(".stackbanner").css({"display":"none"});
}
});
</script>
