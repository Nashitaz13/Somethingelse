(function($){

    var location = window.location.href.replace(/\/$|\/?\?.*$/g, '');

    $('a[data-active-class]').each(function(index, elem) {
        var $elem = $(elem);

        if ($elem.attr('href') === location) {
            $elem.addClass($elem.data('activeClass'));

            if ($elem.data('parentMenu')) {
                var parentUrl = $elem.data('parentMenu');
                $('a[data-active-class][href="' + window.location.origin +  parentUrl + '"]')
                    .each(function(index, elem) {
                        var $elem = $(elem);
                        $elem.addClass($elem.data('activeClass'));
                    });
            }
        }
    });

})(jQuery);

$(function() {
    if ($('body').hasClass('mobile')) { return false; }

    var top_menu_offset = 175;
    /**
     * This part causes smooth scrolling using scrollto.js
     * We target all a tags inside the nav, and apply the scrollto.js to it.
     */
    $(".js-sticky-navigation-item a").click(function(e) {
        e.preventDefault();
        var js_id = this.hash.replace('#', '.js-');
        $('html,body').scrollTo($(js_id).offset().top - top_menu_offset, 400);
    });

    /**
     * This part handles the highlighting functionality.
     * We use the scroll functionality again, some array creation and
     * manipulation, class adding and class removing, and conditional testing
     */
    var aChildren = $(".js-sticky-navigation").find('a.js-anchor'); // find the a children of the list items
    var aArray = []; // create the empty aArray
    for (var i=0; i < aChildren.length; i++) {
        var aChild = aChildren[i];
        var ahref = $(aChild).attr('href');
        aArray.push(ahref);
    } // this for loop fills the aArray with attribute href values

    $(window).scroll(function() {
        var windowPos = $(window).scrollTop(); // get the offset of the window from the top of page
        var windowHeight = $(window).height(); // get the height of the window
        var docHeight = $(document).height();

        for (var i=0; i < aArray.length; i++) {
            var theID = aArray[i];
            var js_id = theID.replace('#', '.js-');
            var divPos = $(js_id).offset().top - top_menu_offset; // get the offset of the div from the top of page
            var divHeight = $(js_id).height(); // get the height of the div in question
            if (windowPos >= divPos-5 && windowPos < (divPos + divHeight)) {
                $("a[href='" + theID + "']").parent().addClass("sticky-navigation__item_state_active");
            } else {
                $("a[href='" + theID + "']").parent().removeClass("sticky-navigation__item_state_active");
            }
        }

        if (windowPos + windowHeight == docHeight) {
            if (!$(".js-sticky-navigation-item").last().hasClass("sticky-navigation__item_state_active")) {
                $(".sticky-navigation__item_state_active").removeClass("sticky-navigation__item_state_active");
                $(".js-sticky-navigation-item").last().addClass("sticky-navigation__item_state_active");
            }
        }
    });

    var limitExecByInterval = function(fn, time) {
	var lock, args;
    	return function() {
    		args = arguments;
    		if (!lock) {
    			lock = true;
    			var scope = this;
    			setTimeout(function(){
    				lock = false;
    			}, time);
    			return fn.apply(this, args);
    		}
    	};
    };

    $(".js-sticky-navigation-arrow-up").click(limitExecByInterval(function(e) {
        e.preventDefault();
            $('html,body').scrollTo(0, 400);
    }, 400));

    $(".js-sticky-navigation-arrow-down").click(limitExecByInterval(function(e) {
        e.preventDefault();
        $('html,body').scrollTo('100%', 400);
    }, 400));


});

$(function() {
    if ($('body').hasClass('mobile')) { return false; }
    var $bigCounter = $('.js-teasing-counter');

    if ($bigCounter.length) {
        var $digit = $('.big-counter__digit');
        var currentTime = new Date(store.serverCurrentTimestamp*1000);
        var targetTime = new Date($bigCounter.data('timetarget'));
        var diffTime = new Date(currentTime - (new Date));

        var timer = setInterval(function() {
            var time = new Date(targetTime.getTime() - new Date((new Date).getTime() + diffTime.getTime()).getTime());

            if (time.getTime() > 0) {
                var timeString = ('0'+Math.floor(time.getTime() / (24 * 60 * 60 * 1000))).slice(-2);
                timeString += ('0'+time.getUTCHours()).slice(-2);
                timeString += ('0'+time.getUTCMinutes()).slice(-2);
                timeString += ('0'+time.getUTCSeconds()).slice(-2);

                $digit.each(function(id) {
                    $(this).text(timeString[id]);
                });
            }
            else {
                clearInterval(timer);
            }


        }, 1000);

    }

    var $signup = $('.js-teasing-send-email-form'),
        $email = $('.js-teasing-email');

    if ($signup.length) {
        $signup.submit(function(e) {
            e.preventDefault();
            var email = $email.val();

            if (email.match(/^[-a-z0-9~!$%^&*_=+}{\'?]+(\.[-a-z0-9~!$%^&*_=+}{\'?]+)*@([a-z0-9_][-a-z0-9_]*(\.[-a-z0-9_]+)*\.(aero|arpa|biz|com|coop|edu|gov|info|int|mil|museum|name|net|org|pro|travel|mobi|[a-z][a-z])|([0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}))(:[0-9]{1,5})?$/i)) {
                $.ajax({
                    url: '/index/newsletter/',
                    data: {
                        YII_CSRF_TOKEN: (typeof Rocket !== 'undefined' && Rocket.helper.csrf.getToken()) || '',
                        NewsletterSignupForm: {
                            email: email
                        },
                        isAjax: 1,
                        subscription: 'men'
                    },
                    dataType: 'json',
                    success: function(data) {
                        data = data.data;

                        if (data) {
                            if (data.success) {
                                $signup.find('input').hide();
                                $signup.find('.js-teasing-send-email-form__wrap').addClass('teasing-send-email_subscribed').find('.js-newsletter-message').append(data.message);
                            }
                            else if(data.email) {
                                alert(data.email);
                            }
                            else {
                                alert(translate('Server error, please try again.'));
                            }
                        }
                        else {
                            alert(translate('Server error, please try again.'));
                        }
                    },
                    error: function() {
                        alert(translate('Server error, please try again.'));
                    }
                });
            }
            else {
                alert(translate('Enter a valid email.'));
            }
        });
    }

    $('.js-tvc').nyroModal();
});
$(window).load(function(){
	$(".product-badge .product-badge__price").each(function(){
		var tempPrice = parseInt($(this).text());
		$(this).html(tempPrice.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".")+ " VND");
	});
});
