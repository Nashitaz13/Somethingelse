$(document).ready(function(){
/* Add noted for tkb */




function setCookieHoliday(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays*24*60*60*1000));
    var expires = "expires="+d.toUTCString();
    document.cookie = cname + "=" + cvalue + ";path=/;" + expires;
}
function getCookieHoliday(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
    }
}
if(($('.product__detail-badge').hasClass('t_w1') || $('.product__detail-badge').hasClass('t_w2') || $('.product__detail-badge').hasClass('t_w3')) && (getCookieHoliday("checkGoon")!="goon")) {
$("body").append('<div class="popup legal-popup"><table><tr><td><div class="popup-inner"><div class="popup-content"><div class="legal-content" style="text-align:left;font-size:13px;"><p>Thông tin mô tả tại đây chỉ mang tính chất trợ giúp người đọc hiểu hơn về sản phẩm, không nhằm mục đích quảng cáo.</p><p>Vui lòng xác nhận bạn là nhân viên y tế và/hoặc có nhu cầu tìm hiểu về sản phẩm.</p></div><div class="legal-element"><input id="legal-cb" type="checkbox" checked><label for="legal-cb">Không hiển thị thông báo này lần sau</label></div><div class="list-button"><a href="" class="legal-goon" onclick="return false;">Xác nhận</a><a href="http://www.lazada.vn" class="legal-ignored"><span>Bỏ qua</span></a></div></div></div></td></tr></table></div>');
}
$(".legal-goon").click(function(){
        if($("#legal-cb").is(":checked")){
           setCookieHoliday("checkGoon", "goon", 1);
        }
        $(".legal-popup").fadeOut();
});




var lastPriceString = $('#special_price_box').text().replace('VND','').replace(/\./g, '').trim();
var lastPriceInt = parseInt(lastPriceString);

try {
    /* Deatail page */
    if($('.product__detail-badge').hasClass('re_10')) {
        if(lastPriceInt > 10000000) {
            var newPrice = lastPriceInt - 1000000;
        }
        else {
            var newPrice = lastPriceInt - lastPriceInt * 0.1;
        }
        $('#special_price_box').html((Math.round(newPrice/100)*100).toString().replace(/\B(?=(\d{3})+(?!\d))/g, "."));
    }
    /* Catalog page */
    $('.product-card.re_10 .product-card__price').each(function() {
        var dlastPriceString = $(this).text().replace('VND','').replace(/\./g, '').trim();
        var dlastPriceInt = parseInt(dlastPriceString);
        var dnewPrice;
        if(dlastPriceInt > 10000000) {
            dnewPrice = dlastPriceInt - 1000000;
        }
        else {
            dnewPrice = dlastPriceInt - dlastPriceInt * 0.1;
        }

        $(this).html((Math.round(dnewPrice/100)*100).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' VND');
    });
}
catch(err) {
    
}



if($('.product__detail-badge').hasClass('p_m')) {
	$('.prod_brief').append('<div class="warranty-more">>> Xem thêm nhiều lựa chọn khu vực giao hàng</div>');
}
$(".warranty-more").click(function(){
$('body,html').animate({
    scrollTop: $('.product-multisource').offset().top - 55 + 'px'
  }, 800);
});

if($('.product__detail-badge').hasClass('bi_ti')) {
   $("#product_saving_percentage").html("15%");
}



if($('.product__detail-badge').hasClass('fri_10')) {

     newPrice = lastPriceInt - lastPriceInt * 0.1;

    $('#product-price-box .prod_pricebox_price').before('<div class="mobile-block"><div class="dnew-price-all dnew-price-10"><div class="np-left"><div class="np-left-title">Nhập mã</div>   <div class="np-voucher">FRISO1</div></div><div class="np-right"><div class="np-left-title">Giảm còn</div><div class="np-prize">' + (Math.floor(newPrice/100)*100).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + ' VND' + '</div></div></div></div>' );
}



function searchalld(searchType){
var DEFAULT_TEXT = {};



	return DEFAULT_TEXT[searchType]; 
}




var sku = $('#pdtsku').text();

var newpriced = searchalld(sku);
if(newpriced != undefined)
{

$('#product-price-box .prod_pricebox_price').next('.mobile-block').remove();
$('#product-price-box .prod_pricebox_price').prev('.mobile-block').remove();
$('#product-price-box .prod_pricebox_price').after('<div class="mobile-block"><div class="sticker-visa">' + newpriced + '</div></div>');	
}

