})(jQuery);

if (typeof getProductsURL === 'undefined') {
    var getProductsURL = '/catalog/ajaxGetSkus';
}

$(function() {
    if ($('body').hasClass('mobile')) { return false; }
    var earlyDealsSkus = $('#js-early-deals-skus');
    var $tpl = $('#js-early-deals-item');
    var $dealsWrap = $('.js-early-deals__wrap');

    var currencyFormat = {
        "ID": {
            format: "RP #",
            decimal: 2
        },
        "MY": {
            format: "RM #",
            decimal: 2
        },
        "PH": {
            format: "₱ #",
            decimal: 0
        },
        "TH": {
            format: "# บาท",
            decimal: 2
        },
        "VN": {
            format: "# VND",
            decimal: 0
        },
        "SG": {
            format: "SGD #",
            decimal: 2
        }
    };
    var curFormatVenture = currencyFormat[store.venture];

    if (earlyDealsSkus.length && $tpl.length && $dealsWrap.length) {
        var tpl = $tpl.html();
        earlyDealsSkus = earlyDealsSkus.html();
        earlyDealsSkus = JSON.parse(earlyDealsSkus);

        if (earlyDealsSkus && earlyDealsSkus.length) {
            var reqSkus = [];
            var len = earlyDealsSkus.length;
            for (var i = 0; i < len; i++) {
                reqSkus = reqSkus.concat(earlyDealsSkus[i].skus);
            }
        }

        if(reqSkus.length) {

            getProductsBySkus(getProductsURL, reqSkus.join(','), function(products) {
                var len = earlyDealsSkus.length;
                var dealsHTML = '';

                for (var i = 0; i < len; i++) {
                    var skusLen = earlyDealsSkus[i].skus.length;
                    for (var j = 0; j < skusLen; j++) {
                        var prod = products[earlyDealsSkus[i].skus[j]];
                        if (prod && !prod.isOOS) {
                            dealsHTML += Mustache.render(tpl, prepareProduct(prod, earlyDealsSkus[i]));
                            break;
                        }
                    }
                }

                $dealsWrap.html(dealsHTML);
            });
        }
    }

    var skusCfg = $('#js-skus-config');

    if (skusCfg.length) {
        skusCfg = JSON.parse(skusCfg.html());
        if (skusCfg.length) {
            (function() {
                var allSkus = [];
                for (var i = 0; i < skusCfg.length; i++) {
                    if (skusCfg[i] && skusCfg[i].skus && skusCfg[i].skus.length) {
                        for (var j = 0; j < skusCfg[i].skus.length; j++) {
                            allSkus = allSkus.concat(skusCfg[i].skus[j]);
                        }
                    }
                }

                if (allSkus.length) {
                    getProductsBySkus(getProductsURL, allSkus.join(','), function(skus) {
                        for (var i = 0; i < skusCfg.length; i++) {
                            if (skusCfg[i] && skusCfg[i].skus && skusCfg[i].skus.length) {
                                var $wrap = $(skusCfg[i].wrap);
                                var $tpl = $(skusCfg[i]['item-tpl']);

                                if ($wrap.length && $tpl.length) {
                                    var tpl = $tpl.html();
                                    if (skusCfg[i].type === 'multiple') {
                                        $wrap.each(function(j) {
                                            if (skusCfg[i].skus[j] && skusCfg[i].skus[j].length) {
                                                var currSkus = skusCfg[i].skus[j];
                                                var count = 0, limit = skusCfg[i].limit;
                                                var resultHTML = '';

                                                for (var y = 0; y < currSkus.length; y++) {
                                                    if (skus[currSkus[y]] && !skus[currSkus[y]].isOOS) {
                                                        resultHTML += Mustache.render(tpl, prepareProduct(skus[currSkus[y]]));
                                                        count++;
                                                        if (count === limit) {
                                                            break;
                                                        }
                                                    }
                                                }

                                                $(this).html(resultHTML);
                                            }
                                        });
                                    }
                                }


                                for (var j = 0; j < skusCfg[i].skus.length; j++) {
                                    skusCfg[i].skus[j];
                                }
                            }
                        }
                    });
                }


            })();
        }
    }

    function prepareProduct(prod, earlyDealsSku) {
        return {
            url: '/' + prod.link,
            sku: prod.sku,
            cat_name: earlyDealsSku? earlyDealsSku.title : '',
            image: prod.image+'-catalog.jpg',
            name: prod.name,
            specialPrice: curFormatVenture.format.replace('#', formatMoneyExt(prod.special_price, curFormatVenture.decimal, ',', '.')),
            price: curFormatVenture.format.replace('#', formatMoneyExt(prod.price, curFormatVenture.decimal, ',', '.')),
            savingPercent: prod.max_saving_percentage
        };
    }

    function getProductsBySkus(url, skus, callback) {
        $.ajax({
            dataType: "json",
            url: url,
            data: {
                skus: skus
            },
            success: function(data) {
                var products = {};
                if (data.success) {
                    var len = data.data.length;
                    if (len) {
                        for (var i = 0; i < len; i++) {
                            var prod = data.data[i];
                            products[prod.meta.sku] = prod.meta;
                            products[prod.meta.sku].image = prod.image;
                            products[prod.meta.sku].link = prod.link;
                        }
                    }
                }
                callback(products);
            }
        });
    }

    function formatMoneyExt(n, c, d, t) {
        var c = isNaN(c = Math.abs(c)) ? 2 : c,
            d = d == undefined ? "." : d,
            t = t == undefined ? "," : t,
            s = n < 0 ? "-" : "",
            i = parseInt(n = Math.abs(+n || 0).toFixed(c)) + "",
            j = (j = i.length) > 3 ? j % 3 : 0;
        return s + (j ? i.substr(0, j) + t : "") + i.substr(j).replace(/(\d{3})(?=\d)/g, "$1" + t) + (c ? d + Math.abs(n - i).toFixed(c).slice(2) : "");
    };
});
if (typeof getProductsURL === 'undefined') {
    var getProductsURL = '/catalog/ajaxGetSkus';
}

if (typeof getPricesURL === 'undefined') {
    var getPricesURL = '/ajax/catalog/getFlashsalePrices/';
}

var timeCurrent;

$(function() {
    if ($('body').hasClass('mobile')) { return false; }
    var rotateSpeed = 3000;

    var currencyFormat = {
        "ID": {
            format: "RP #",
            decimal: 2
        },
        "MY": {
            format: "RM #",
            decimal: 2
        },
        "PH": {
            format: "₱ #",
            decimal: 0
        },
        "TH": {
            format: "# บาท",
            decimal: 2
        },
        "VN": {
            format: "# VND",
            decimal: 0
        },
        "SG": {
            format: "SGD #",
            decimal: 2
        }
    };
    var curFormatVenture = currencyFormat[store.venture];

    function Slider($elem, $leftArr, $rightArr, rotate, itemSelector) {
        var that = this,
            positions = {
                left: 0,
                wrapWidth: 0,
                sliderWidth: 0,
                elemWidth: 0,
                mouseX: 0
            },
            callbacksOnMove = [];


        this._itemSelector = itemSelector;

        $elem.find('img').on('dragstart', function(event) { event.preventDefault(); });

        this.listenMove = function(callback) {
            callbacksOnMove.push(callback);
        };

        this._callListeners = function (left) {
            var currPos = this._getPos(),
                position = {
                    left: left,
                    wrapWidth: currPos.wrapWidth,
                    sliderWidth: currPos.sliderWidth,
                    elemWidth: currPos.elemWidth,
                    mouseX: currPos.mouseX
                };

            for (var i = 0; i < callbacksOnMove.length; i++) {
                callbacksOnMove[i](position);
            }
        }

        this._getPos = function() {
            return positions;
        }

        this._setPos = function(pos) {
            if (pos.left !== undefined)         { positions.left        = pos.left; }
            if (pos.wrapWidth !== undefined)    { positions.wrapWidth   = pos.wrapWidth; }
            if (pos.sliderWidth !== undefined)  { positions.sliderWidth = pos.sliderWidth; }
            if (pos.elemWidth !== undefined)    { positions.elemWidth   = pos.elemWidth; }
            if (pos.mouseX !== undefined)       { positions.mouseX      = pos.mouseX; }
        }

        this._getElem = function() {
            return $elem
        }

        this.calcPosition();
        if (this._getPos().sliderWidth >= this._getPos().wrapWidth) {
            this._getElem().css({ left: this._getPos().left });
            $leftArr.on('click touchend', function (e) {
                that.calcPosition(e);
                that.stopRotate();
                var pos = that._getPos(),
                    left = pos.left + pos.elemWidth;

                if (left > 0) {
                    left = pos.wrapWidth - pos.sliderWidth;
                }

                left = Math.round(left/pos.elemWidth)*pos.elemWidth;

                that._getElem().css({ left: left });
                that._callListeners(left);
            });

            $rightArr.on('click touchend', function (e) {
                that.calcPosition(e);
                that.stopRotate();
                var pos = that._getPos(),
                    left = pos.left - pos.elemWidth;

                if ((pos.sliderWidth + left) < pos.wrapWidth) {
                    left = 0;
                }

                left = Math.round(left/pos.elemWidth)*pos.elemWidth;

                that._getElem().css({ left: left });
                that._callListeners(left);
            });

            this._onStart = function(e) { that.onStart(e) };
            this._onEnd = function(e) { that.onEnd(e) };
            this._onMove = function(e) { that.onMove(e) };

            this._getElem().on('touchstart mousedown', this._onStart);

            if (rotate) {
                rotate = setInterval(function() {
                    that.calcPosition();
                    var pos = that._getPos(),
                    left = pos.left - pos.elemWidth;

                    if ((pos.sliderWidth + left) < pos.wrapWidth) {
                        left = 0;
                    }

                    that._getElem().css({ left: left });
                }, rotate);
            }
        }

        this.stopRotate = function () {
            if (rotate) {
                clearInterval(rotate);
            }
        }
    }

    Slider.prototype.initActions = function () {
        this._getElem()
            .on('touchend touchcancel mouseup', this._onEnd)
            .on('touchmove mousemove', this._onMove);
    }

    Slider.prototype.unbindActions = function ($elem) {
        this._getElem()
            .off('touchend touchcancel mouseup', this._onEnd)
            .off('touchmove mousemove', this._onMove);
    }

    Slider.prototype.destruct = function () {
        this.unbindActions();
        this._getElem().css({ left: 0 })
            .off('touchstart mousedown', this._onStart);
        this.stopRotate();
    }

    Slider.prototype.getX = function (e) {
        return (e.originalEvent && (e.originalEvent.touches && e.originalEvent.touches[0] && e.originalEvent.touches[0].pageX)
                || (e.originalEvent.changedTouches && e.originalEvent.changedTouches[0] && e.originalEvent.changedTouches[0].pageX))
                || e.pageX;
    }

    Slider.prototype.calcPosition = function(e) {
        var $elem = this._getElem(),
            $wrap = $elem.parent();

        this._setPos({
            left: $elem.position().left,
            mouseX: (e && this.getX(e)),
            wrapWidth: $wrap.width(),
            sliderWidth: $elem.outerWidth(true),
            elemWidth: this._itemSelector? $elem.find(this._itemSelector).outerWidth(true) : $elem.children().outerWidth(true)
        });
    }

    Slider.prototype.onStart = function (e) {
        this.stopRotate();
        this.calcPosition(e);
        this.initActions();
    }

    Slider.prototype.onEnd = function (e) {
        var pos = this._getPos();
        var left = Math.round(this._getElem().position().left/pos.elemWidth)*pos.elemWidth;
        this._getElem().css({ left: left });

        this.unbindActions();
        this._callListeners(left);
    }

    Slider.prototype.onMove = function (e) {
        var pos = this._getPos(),
            x = this.getX(e),
            left;

        if (pos.mouseX !== x) {
            left = pos.left + (x - pos.mouseX);
            if (left < 0 && (pos.sliderWidth + left) > pos.wrapWidth) {
                this._getElem().css({ left: left });
            }
        }
    }

    if (typeof timeCurrent === 'undefined') {
        if ($('.js-flashsales-banner').data('current')) {
            timeCurrent = new Date($('.js-flashsales-banner').data('current')*1000);
        }
        else if ($('.js-active-sale').data('current')) {
            timeCurrent = new Date($('.js-active-sale').data('current')*1000);
        }
    }
    
    var timeDiff = timeCurrent - (new Date);
    var $countdownPlaceholder = $('.js-sale-timer');
    var $currentSaleContent = $('.js-sale-content');
    var globalInterval, headFlashInterval;

    var flashsalesSkusConfig = $('#flashsales-skus');
    var $productsWrapTpl = $('#js-flashsales-products-wrap-tpl');
    var $comingWrap = $('.js-coming-sales-wrap');
    var $flashHeadTpl = $('#js-flash-head-tpl');
    var $flashHeadWrap = $('.js-flash-head__wrap');
    var $flashHeadTimer = $('.js-flash-head__timer');
    var flashPricesRequest = [];

    if (flashsalesSkusConfig.length) {
        flashsalesSkusConfig = flashsalesSkusConfig.html();
        flashsalesSkusConfig = JSON.parse(flashsalesSkusConfig);
    }

    if (flashsalesSkusConfig && flashsalesSkusConfig.flashSales 
        && flashsalesSkusConfig.flashSales.length 
        && ($productsWrapTpl.length || $flashHeadTpl.length)) {

        buildPriceRequest(flashsalesSkusConfig.flashSales);

        if (flashsalesSkusConfig.showPromotionalPrices) {
            getFlashPrices(buildFlashsalesConfig);
        }
        else
        {
            buildFlashsalesConfig();
        }
    }

    function getFlashPrices(callback, time) {
        var request = [];
        if (time) {
            for (var i = 0; i < flashPricesRequest.length; i++) {
                if (flashPricesRequest[i].start === time.start && flashPricesRequest[i].end === time.end) {
                    request.push(flashPricesRequest[i]);
                    break;
                }
            }
        }
        else {
            request = flashPricesRequest;
        }

        $.ajax({
            url: getPricesURL,
            type: "POST",
            data: JSON.stringify(request),
            contentType: "application/json",
            dataType: "json",
            success: function(data) {
                if (data.data && data.data.length) {
                    var res = {};
                    for (var i = 0; i < data.data.length; i++) {
                        res[data.data[i].start+data.data[i].end] = data.data[i].special_price;
                    }
                    callback(res);
                }
            },
            processData: false
        });
    }

    function buildPriceRequest(cfg) {
        var len  = cfg.length;

        for (var i = 0; i < len; i++) {
            var tmpCfg = {};
            tmpCfg.start = cfg[i].time.start;
            tmpCfg.end = cfg[i].time.end;
            tmpCfg.skus = [];

            for (var j = 0; j < cfg[i].skus.length; j++) {
                tmpCfg.skus.push(cfg[i].skus[j].sku);
            }

            flashPricesRequest.push(tmpCfg);
        }
    }

    function buildFlashsalesConfig(prices) {
        var flashsalesSkus = flashsalesSkusConfig.flashSales;
        var productsTpl = $productsWrapTpl.html();
        var reqSkus = [];
        var len = flashsalesSkus.length;

        for (var i = 0; i < len; i++) {
            var lenSkus = flashsalesSkus[i].skus.length;
            for (var j = 0; j < lenSkus; j++) {
                reqSkus.push(flashsalesSkus[i].skus[j].sku);
            }
        }

        if(reqSkus.length) {
            getProductsBySkus(getProductsURL, reqSkus.join(','), function(products) {
                var len = flashsalesSkus.length;
                var saleHTML = '';

                for (var i = 0; i < len; i++) {
                    var skusLen = flashsalesSkus[i].skus.length;
                    var prods = [];

                    for (var j = 0; j < skusLen; j++) {
                        if (products[flashsalesSkus[i].skus[j].sku] && !products[flashsalesSkus[i].skus[j].sku].isOOS) {
                            var tmpProd = JSON.parse(JSON.stringify(products[flashsalesSkus[i].skus[j].sku]));
                            var promPrice = (prices && prices[flashsalesSkus[i].time.start+flashsalesSkus[i].time.end] 
                                                        && prices[flashsalesSkus[i].time.start+flashsalesSkus[i].time.end][flashsalesSkus[i].skus[j].sku]);
                            if (promPrice === '') {
                                promPrice = null;
                            }

                            if (promPrice) {
                                tmpProd.max_saving_percentage = getPercent(tmpProd.price, promPrice);
                            } else {
                                promPrice = flashsalesSkusConfig.showInsteadSpecialPrice;
                                tmpProd.max_saving_percentage = null;
                            }

                            tmpProd.special_price = promPrice;
                            tmpProd.hideBuy = flashsalesSkus[i].skus[j].hideBuy;
                            prods.push(tmpProd);
                        }
                    }

                    var dataForTpl = {
                        time: {
                            startAMPM: formatAMPM(new Date(flashsalesSkus[i].time.start)),
                            endAMPM: formatAMPM(new Date(flashsalesSkus[i].time.end)),
                            start: flashsalesSkus[i].time.start,
                            end: flashsalesSkus[i].time.end
                        },
                        products: prepareProducts(prods)
                    };

                    if ($flashHeadTpl.length) {
                        initFlashHead(dataForTpl);
                    }

                    if ($productsWrapTpl.length) {
                        saleHTML += Mustache.render(productsTpl, dataForTpl);
                    }
                }

                if ($productsWrapTpl.length) {
                    $comingWrap.html(saleHTML);
                    initProducts();
                }
            });
        }
    }

    function initFlashHead(data) {
        var timeStart = new Date(data.time.start);
        var timeEnd = new Date(data.time.end);

        if (timeCurrent < timeStart) {
            setTimeout(function() {
                buildProdsForFlashHead(timeEnd, data);
            }, (timeStart - timeCurrent));
        }
        else if (timeCurrent < timeEnd) {
            buildProdsForFlashHead(timeEnd, data);
        }
    }

    function buildProdsForFlashHead(timeEnd, data) {
        if (headFlashInterval) {
            clearInterval(headFlashInterval);
        }

        $flashHeadWrap.html(Mustache.render($flashHeadTpl.html(), data));
        new Slider($flashHeadWrap.find('.js-slider'),
                    $flashHeadWrap.find('.js-arrow-left'),
                    $flashHeadWrap.find('.js-arrow-right'), rotateSpeed);

        $flashHeadTimer.html(calcCountdown(timeEnd));

        var currInterval = setInterval(function() {
            var countDown = calcCountdown(timeEnd);
            if (countDown) {
                $flashHeadTimer.html(countDown);
            }
            else {
                clearInterval(currInterval);
                $flashHeadWrap.html('');
            }
        }, 1000);

        headFlashInterval = currInterval;

        if (!flashsalesSkusConfig.showPromotionalPrices) {
            getFlashPrices(function(prices) {
                var currPrices = prices[data.time.start+data.time.end];
                var skus = Object.keys(currPrices);
                for (var i = 0; i < skus.length; i++) {
                    if (currPrices[skus[i]]) {
                        $flashHeadWrap.find('.js-head-product_'+skus[i]+' .js-head-product-special-price').html(curFormatVenture.format.replace('#', formatMoneyExt(currPrices[skus[i]], curFormatVenture.decimal, '.', ',')));
                        $flashHeadWrap.find('.js-head-product_'+skus[i]+' .js-discount').removeClass('hidden').find('.js-discount-value').html(getPercent($flashHeadWrap.find('.js-head-product_'+skus[i]).data('rawprice'), currPrices[skus[i]]));
                    }
                    else {
                        $flashHeadWrap.find('.js-head-product_'+skus[i]+' .js-product-special_price').html(flashsalesSkusConfig.showInsteadSpecialPrice);
                    }
                }

            }, data.time);
        }
    }

    function initProducts() {
        $('.js-products').each(function(i) {
            var $prod = $(this);
            var timeStart = new Date($prod.data('start'));
            var timeEnd = new Date($prod.data('end'));

            if (timeCurrent < timeStart) {
                $prod.show();
                var slider = new Slider($prod.find('.js-slider'), $prod.find('.js-arrow-left'), $prod.find('.js-arrow-right'));

                setTimeout(function() {
                    slider.destruct();
                    moveProducts($currentSaleContent, $prod, timeEnd, timeCurrent);
                }, (timeStart - timeCurrent));
            }
            else if (timeCurrent < timeEnd) {
                moveProducts($currentSaleContent, $prod, timeEnd, timeCurrent);
            }
            else {
                $prod.hide();
            }
        });
    }

    function moveProducts($currentSaleContent, $prod, timeEnd, timeCurrent) {
        var prodContent = '';
        var $buyButton;
        var time = { start: $prod.data('start'), end:  $prod.data('end') };

        $prod.find('.js-product').each(function() {
            prodContent += $(this).html();
        });
        $currentSaleContent.html(prodContent);
        $prod.remove();
        $countdownPlaceholder.html(calcCountdown(timeEnd));
        $buyButton = $currentSaleContent.find('.js-button-buy');
        $buyButton.removeClass('hidden');
        $buyButton.each(function() {
            var $buy = $(this);
            if (!$buy.data('nocart')) {
                window.Shop && $buy.RocketQuickbuy(window.Shop.cfg.quickbuy);
            }
        });
        startTimer($countdownPlaceholder, timeEnd);

        if (!flashsalesSkusConfig.showPromotionalPrices) {
            getFlashPrices(function(prices) {
                var currPrices = prices[time.start+time.end];
                var skus = Object.keys(currPrices);
                for (var i = 0; i < skus.length; i++) {
                    if (currPrices[skus[i]]) {
                        $currentSaleContent.find('.js-product_'+skus[i]+' .js-product-special_price').html(curFormatVenture.format.replace('#', formatMoneyExt(currPrices[skus[i]], curFormatVenture.decimal, '.', ',')));
                        $currentSaleContent.find('.js-product_'+skus[i]+' .js-discount').removeClass('hidden').find('.js-discount-value').html(getPercent($('.js-product_'+skus[i]).data('rawprice'), currPrices[skus[i]]));
                    }
                    else {
                        $currentSaleContent.find('.js-product_'+skus[i]+' .js-product-special_price').html(flashsalesSkusConfig.showInsteadSpecialPrice);
                    }
                }

            }, time);
        }
    }

    function startTimer($countdownPlaceholder, timeEnd) {
        clearInterval(globalInterval);
        var currInterval = setInterval(function() {
            var countDown = calcCountdown(timeEnd);
            if (countDown) {
                $countdownPlaceholder.html(countDown);
            }
            else {
                clearInterval(currInterval);
            }
        }, 1000);
        globalInterval = currInterval;
    }

    // setInterval(getCountdown, 1000);
    // var current = new Date((new Date) + timeDiff);
    function calcCountdown(target) {
        return getCountdown(target, new Date((new Date).getTime() + timeDiff));
    }

    function getCountdown(target, current) {
        var diff = new Date(target - current),
            result = false;

        if (diff > 0) {
            result = ('0'+diff.getUTCHours()).slice(-2)+':'
                        +('0'+diff.getUTCMinutes()).slice(-2)+':'
                        +('0'+diff.getUTCSeconds()).slice(-2);
        }

        return result;
    }

    function formatAMPM(date) {
        var hours = date.getHours();
        var ampm = hours >= 12 ? 'pm' : 'am';
        hours = hours % 12;
        hours = hours ? hours : 12; // the hour '0' should be '12'
        var strTime = hours + ampm;
        return strTime;
    }

    function prepareProducts(prods) {
        var res = [];
        var len = prods.length;

        for (var i = 0; i < len; i++) {
            res.push({
                link: '/' + prods[i].link,
                url: '/' + prods[i].link,
                sku: prods[i].sku,
                image: prods[i].image+'-catalog.jpg',
                name: prods[i].name,
                hideBuy: prods[i].hideBuy,
                rawprice: prods[i].price,
                specialPrice: (flashsalesSkusConfig.showInsteadSpecialPrice === prods[i].special_price)? prods[i].special_price : (!flashsalesSkusConfig.showPromotionalPrices && flashsalesSkusConfig.showInsteadSpecialPrice) || (prods[i].special_price && curFormatVenture.format.replace('#', formatMoneyExt(prods[i].special_price, curFormatVenture.decimal, '.', ','))),
                price: curFormatVenture.format.replace('#', formatMoneyExt(prods[i].price, curFormatVenture.decimal, '.', ',')),
                savingPercent: prods[i].max_saving_percentage,
                simpleSku: prods[i].simpleSku
            });
        }

        return res;
    }

    function prepareProduct(prod, earlyDealsSku) {
        return {
            url: '/' + prod.link,
            link: '/' + prod.link,
            sku: prod.sku,
            cat_name: earlyDealsSku? earlyDealsSku.title : '',
            image: prod.image+'-catalog.jpg',
            name: prod.name,
            specialPrice: prod.special_price && curFormatVenture.format.replace('#', formatMoneyExt(prod.special_price, curFormatVenture.decimal, '.', ',')),
            price: curFormatVenture.format.replace('#', formatMoneyExt(prod.price, curFormatVenture.decimal, '.', ',')),
            savingPercent: prod.max_saving_percentage
        };
    }

    function getProductsBySkus(url, skus, callback) {
        $.ajax({
            dataType: "json",
            url: url,
            data: {
                skus: skus
            },
            success: function(data) {
                var products = {};
                if (data.success) {
                    var len = data.data.length;
                    if (len) {
                        for (var i = 0; i < len; i++) {
                            var prod = data.data[i];
                            products[prod.meta.sku] = prod.meta;
                            products[prod.meta.sku].image = prod.image;
                            products[prod.meta.sku].link = prod.link;
                            products[prod.meta.sku].simpleSku = (prod.simple && prod.simple.meta && prod.simple.meta.sku) || '';
                        }
                    }
                }
                callback(products);
            }
        });
    }

    function getPercent(oldPrice, newPrice) {
        oldPrice = oldPrice*1;
        newPrice = newPrice*1;

        return (((oldPrice - newPrice) / oldPrice) * 100).toFixed();
    }

    function formatMoneyExt(n, c, d, t) {
        var c = isNaN(c = Math.abs(c)) ? 2 : c,
            d = d == undefined ? "." : d,
            t = t == undefined ? "," : t,
            s = n < 0 ? "-" : "",
            i = parseInt(n = Math.abs(+n || 0).toFixed(c)) + "",
            j = (j = i.length) > 3 ? j % 3 : 0;
        return s + (j ? i.substr(0, j) + t : "") + i.substr(j).replace(/(\d{3})(?=\d)/g, "$1" + t) + (c ? d + Math.abs(n - i).toFixed(c).slice(2) : "");
    };

    var $sliders = $('.js-simple-slider');
    if ($sliders.length) {
        $sliders.each(function(i) {
            var $elem = $(this);
            var speed = +$elem.data('speed') || rotateSpeed;
            $elem.data('slider', new Slider($elem.find('.js-slider-type'), $elem.find('.js-slider-arrow-left'), $elem.find('.js-slider-arrow-right'), speed));
        });
    }

    var $cmsSliders = $('.js-simple-slider-cms');
    if ($cmsSliders.length) {
        $cmsSliders.each(function(i) {
            var $elem = $(this);
            var speed = +$elem.data('speed') || rotateSpeed;
            $elem.data('slider', new Slider($elem.find('.js-slider-type'), $elem.find('.js-slider-arrow-left'), $elem.find('.js-slider-arrow-right'), speed, '.unit'));
        });
    }

    var dailySaleConfig = $("#js-skus-daily-sale");
    var $dailySaleWrap = $('.js-daily-sale-wrap');
    var dailySaleProductTpl = $('#js-daily-sale-item').html();

    if (dailySaleConfig.length && dailySaleProductTpl && $dailySaleWrap.length) {
        dailySaleConfig = JSON.parse(dailySaleConfig.html());
        if (dailySaleConfig.length) {
            $dailySaleWrap.each(function(id) {
                if (dailySaleConfig[id] && dailySaleConfig[id].skus && dailySaleConfig[id].skus.length) {
                    var skus = dailySaleConfig[id].skus;
                    var showSkus;
                    var $wrap = $(this);
                    var slider;
                    var loadingProducts = false;

                    (function LoadDailyProducts() {
                        if (!loadingProducts) {
                            showSkus = skus.splice(0, 10);
                            loadingProducts = true;
                            if (showSkus.length) {
                                getProductsBySkus(getProductsURL, showSkus.join(','), function(prods) {
                                    var resultHTML = '';
                                    for (var i = 0; i < showSkus.length; i++) {
                                        resultHTML += Mustache.render(dailySaleProductTpl, prepareProduct(prods[showSkus[i]]));
                                    }
                                    $wrap.append(resultHTML);
                                    if (!slider) {
                                        var $slider = $wrap.closest('.js-daily-sale-slider');
                                        slider = new Slider($wrap, $slider.find('.js-slider-arrow-left'), $slider.find('.js-daily-sale-arrow-right'));
                                        slider.listenMove(function(pos) {
                                            if ((pos.sliderWidth + pos.left) <= (pos.elemWidth * 10) ) {
                                                LoadDailyProducts();
                                            }
                                        });
                                    }
                                    loadingProducts = false;

                                    $.ajax({
                                        url: getPricesURL,
                                        type: "POST",
                                        data: JSON.stringify({
                                            time: {
                                                start: dailySaleConfig[id].time.start,
                                                end: dailySaleConfig[id].time.end
                                            },
                                            skus: showSkus
                                        }),
                                        contentType: "application/json",
                                        dataType: "json",
                                        success: function(data) {
                                            if (data.success && data.data && data.data[0] && data.data[0].special_price) {
                                                var prices = data.data[0].special_price;
                                                var skus = Object.keys(prices);

                                                for (var i = 0; i < skus.length; i++) {
                                                    $('.js-item_'+skus[i]+' .js-item-price').html(curFormatVenture.format.replace('#', formatMoneyExt(prices[skus[i]], curFormatVenture.decimal, '.', ',')));
                                                }
                                            }
                                        },
                                        processData: false
                                    });
                                });
                            }
                        }
                    })();
                }
            });
        }
    }

});

/*!
 * jQuery.scrollTo
 * Copyright (c) 2007-2015 Ariel Flesler - aflesler<a>gmail<d>com | http://flesler.blogspot.com
 * Licensed under MIT
 * http://flesler.blogspot.com/2007/10/jqueryscrollto.html
 * @projectDescription Lightweight, cross-browser and highly customizable animated scrolling with jQuery
 * @author Ariel Flesler
 * @version 2.1.2
 */
;(function(factory) {
	'use strict';
	if (typeof define === 'function' && define.amd) {
		// AMD
		define(['jquery'], factory);
	} else if (typeof module !== 'undefined' && module.exports) {
		// CommonJS
		module.exports = factory(require('jquery'));
	} else {
		// Global
		factory(jQuery);
	}
})(function($) {
	'use strict';

	var $scrollTo = $.scrollTo = function(target, duration, settings) {
		return $(window).scrollTo(target, duration, settings);
	};

	$scrollTo.defaults = {
		axis:'xy',
		duration: 0,
		limit:true
	};

	function isWin(elem) {
		return !elem.nodeName ||
			$.inArray(elem.nodeName.toLowerCase(), ['iframe','#document','html','body']) !== -1;
	}		

	$.fn.scrollTo = function(target, duration, settings) {
		if (typeof duration === 'object') {
			settings = duration;
			duration = 0;
		}
		if (typeof settings === 'function') {
			settings = { onAfter:settings };
		}
		if (target === 'max') {
			target = 9e9;
		}

		settings = $.extend({}, $scrollTo.defaults, settings);
		// Speed is still recognized for backwards compatibility
		duration = duration || settings.duration;
		// Make sure the settings are given right
		var queue = settings.queue && settings.axis.length > 1;
		if (queue) {
			// Let's keep the overall duration
			duration /= 2;
		}
		settings.offset = both(settings.offset);
		settings.over = both(settings.over);

		return this.each(function() {
			// Null target yields nothing, just like jQuery does
			if (target === null) return;

			var win = isWin(this),
				elem = win ? this.contentWindow || window : this,
				$elem = $(elem),
				targ = target, 
				attr = {},
				toff;

			switch (typeof targ) {
				// A number will pass the regex
				case 'number':
				case 'string':
					if (/^([+-]=?)?\d+(\.\d+)?(px|%)?$/.test(targ)) {
						targ = both(targ);
						// We are done
						break;
					}
					// Relative/Absolute selector
					targ = win ? $(targ) : $(targ, elem);
					/* falls through */
				case 'object':
					if (targ.length === 0) return;
					// DOMElement / jQuery
					if (targ.is || targ.style) {
						// Get the real position of the target
						toff = (targ = $(targ)).offset();
					}
			}

			var offset = $.isFunction(settings.offset) && settings.offset(elem, targ) || settings.offset;

			$.each(settings.axis.split(''), function(i, axis) {
				var Pos	= axis === 'x' ? 'Left' : 'Top',
					pos = Pos.toLowerCase(),
					key = 'scroll' + Pos,
					prev = $elem[key](),
					max = $scrollTo.max(elem, axis);

				if (toff) {// jQuery / DOMElement
					attr[key] = toff[pos] + (win ? 0 : prev - $elem.offset()[pos]);

					// If it's a dom element, reduce the margin
					if (settings.margin) {
						attr[key] -= parseInt(targ.css('margin'+Pos), 10) || 0;
						attr[key] -= parseInt(targ.css('border'+Pos+'Width'), 10) || 0;
					}

					attr[key] += offset[pos] || 0;

					if (settings.over[pos]) {
						// Scroll to a fraction of its width/height
						attr[key] += targ[axis === 'x'?'width':'height']() * settings.over[pos];
					}
				} else {
					var val = targ[pos];
					// Handle percentage values
					attr[key] = val.slice && val.slice(-1) === '%' ?
						parseFloat(val) / 100 * max
						: val;
				}

				// Number or 'number'
				if (settings.limit && /^\d+$/.test(attr[key])) {
					// Check the limits
					attr[key] = attr[key] <= 0 ? 0 : Math.min(attr[key], max);
				}

				// Don't waste time animating, if there's no need.
				if (!i && settings.axis.length > 1) {
					if (prev === attr[key]) {
						// No animation needed
						attr = {};
					} else if (queue) {
						// Intermediate animation
						animate(settings.onAfterFirst);
						// Don't animate this axis again in the next iteration.
						attr = {};
					}
				}
			});

			animate(settings.onAfter);

			function animate(callback) {
				var opts = $.extend({}, settings, {
					// The queue setting conflicts with animate()
					// Force it to always be true
					queue: true,
					duration: duration,
					complete: callback && function() {
						callback.call(elem, targ, settings);
					}
				});
				$elem.animate(attr, opts);
			}
		});
	};

	// Max scrolling position, works on quirks mode
	// It only fails (not too badly) on IE, quirks mode.
	$scrollTo.max = function(elem, axis) {
		var Dim = axis === 'x' ? 'Width' : 'Height',
			scroll = 'scroll'+Dim;

		if (!isWin(elem))
			return elem[scroll] - $(elem)[Dim.toLowerCase()]();

		var size = 'client' + Dim,
			doc = elem.ownerDocument || elem.document,
			html = doc.documentElement,
			body = doc.body;

		return Math.max(html[scroll], body[scroll]) - Math.min(html[size], body[size]);
	};

	function both(val) {
		return $.isFunction(val) || $.isPlainObject(val) ? val : { top:val, left:val };
	}

	// Add special hooks so that window scroll properties can be animated
	$.Tween.propHooks.scrollLeft = 
	$.Tween.propHooks.scrollTop = {
		get: function(t) {
			return $(t.elem)[t.prop]();
		},
		set: function(t) {
			var curr = this.get(t);
			// If interrupt is true and user scrolled, stop animating
			if (t.options.interrupt && t._last && t._last !== curr) {
				return $(t.elem).stop();
			}
			var next = Math.round(t.now);
			// Don't waste CPU
			// Browsers don't render floating point scroll
			if (curr !== next) {
				$(t.elem)[t.prop](next);
				t._last = this.get(t);
			}
		}
	};

	// AMD requirement
	return $scrollTo;
});

$(function() {
    if ($('body').hasClass('mobile')) { return false; }
    var $placeholders = $('.js-products_placeholder');
    var productTemplate = $('#product_template').html();
    var lazy_placeholder = '';

    var currencyFormat = {
        "ID": {
            format: "RP #",
            decimal: 2
        },
        "MY": {
            format: "RM #",
            decimal: 2
        },
        "PH": {
            format: "₱ #",
            decimal: 0
        },
        "TH": {
            format: "# บาท",
            decimal: 2
        },
        "VN": {
            format: "# VND",
            decimal: 0
        },
        "SG": {
            format: "SGD #",
            decimal: 2
        }
    };
    var curFormatVenture = currencyFormat[store.venture];


    for (var i = 0; i < $placeholders.length; ++i) {
        var $placeholder = $placeholders.eq(i);

        $.getJSON($placeholder.data('url'), (function(idx, $placeholder) {
            return function(data) {
                buildProductsHTML(idx, data, $placeholder);
            };
        })(i, $placeholder));
    }

    function buildProductsHTML(idx, data, $placeholder) {
        if(data.success && data.metadata.results && data.metadata.results.length) {
            var preArray = data.metadata.results;
            var index = 0;
            var targetElemClass, $target, count;

            targetElemClass = $placeholder.data('target');

            $targets = targetElemClass? $placeholder.find('.'+targetElemClass): $placeholder;

            $targets.each(function(id) {
                var $target = $(this);
                var count = parseInt($target.data('limit')) || 5;
                var formatData = data.metadata.results.slice(index, index+count);
                var action;

                if (formatData.length) {
                    formatData = serializeData(formatData);
                    action = $target.data('action');

                    if (action === 'append') {
                        $target.append(Mustache.render(productTemplate, { data: formatData }));
                    }
                    else if (action === 'prepend') {
                        $target.prepend(Mustache.render(productTemplate, { data: formatData }));
                    }
                    else {
                        $target.html(Mustache.render(productTemplate, { data: formatData }));
                    }

                    $target.addClass('js-content_loaded');
                    window.Shop && $target.find('.js-button-buy').RocketQuickbuy(window.Shop.cfg.quickbuy);
                }
                index += count;
            });

            $targets.find('.js-lazyload').lazyload({ placeholder: lazy_placeholder, skip_invisible: false });
        }
    }

    function serializeData(data) {
        var arr_ = [];
        $.each(data, function(idx, obj) {
            var ret = {
                sku: '',
                simpleSku: '',
                name: '',
                url: '',
                brand: '',
                price: '',
                unformatPrice: '',
                max_price: '',
                specialPrice: false,
                savingPercent: false,
                rating: false,
                image: false
            };

            if(typeof obj.data.simples !== 'undefined') {
                var list_simples = [];

                $.each(obj.data.simples, function(key, value) {
                    if (value.meta && value.meta.size) {
                        list_simples.push({
                            size: value.meta.size,
                            quantity: value.meta.quantity,
                            price: value.meta.price,
                            sku: value.meta.sku,
                            parentSku: obj.data.sku
                        });
                    }


                    ret.simpleSku = key;
                });

                ret.has_size = !!list_simples.length;
                if (ret.has_size) {
                    ret.list_simples = list_simples;
                }
                ret.sku = obj.data.sku;
                ret.name = obj.data.name;
                ret.url = obj.data.url.split('/mobapi')[1];
                ret.brand = obj.data.brand;

                ret.unformatPrice = obj.data.price;
                ret.price = curFormatVenture.format.replace('#', formatMoneyExt(obj.data.price, curFormatVenture.decimal, ',', '.'));
                ret.max_price = curFormatVenture.format.replace('#', formatMoneyExt(obj.data.max_price, curFormatVenture.decimal, ',', '.'));
                if (typeof obj.data.special_price !== 'undefined') {
                    ret.specialPrice = curFormatVenture.format.replace('#', formatMoneyExt(obj.data.special_price, curFormatVenture.decimal, ',', '.'));

                    ret.unformatPrice = obj.data.special_price;
                    ret.price = curFormatVenture.format.replace('#', formatMoneyExt(obj.data.special_price, curFormatVenture.decimal, ',', '.'));
                    ret.max_price = curFormatVenture.format.replace('#', formatMoneyExt(obj.data.price, curFormatVenture.decimal, ',', '.'));
                    ret.savingPercent = obj.data.max_saving_percentage;
                }

                if(obj.images.length > 0){
                    ret.image = obj.images[0];
                    if (ret.image.height) {
                        ret.image.height *= 2;
                    }
                    if (ret.image.width) {
                        ret.image.width *= 2;
                    }
                }

                if(typeof obj.data.ratings_total.sum !== 'undefined' && obj.data.ratings_total.sum > 0){
                    ret.rating = {
                        sum: obj.data.ratings_total.sum,
                        avr: obj.data.ratings_total.avr,
                        avrPercent: (obj.data.ratings_total.avr / 5) * 100
                    }
                }
                arr_.push(ret);
            }
        });

        return arr_;
    };

    function formatMoneyExt(n, c, d, t) {
        var c = isNaN(c = Math.abs(c)) ? 2 : c,
            d = d == undefined ? "." : d,
            t = t == undefined ? "," : t,
            s = n < 0 ? "-" : "",
            i = parseInt(n = Math.abs(+n || 0).toFixed(c)) + "",
            j = (j = i.length) > 3 ? j % 3 : 0;
        return s + (j ? i.substr(0, j) + t : "") + i.substr(j).replace(/(\d{3})(?=\d)/g, "$1" + t) + (c ? d + Math.abs(n - i).toFixed(c).slice(2) : "");
    };
});



