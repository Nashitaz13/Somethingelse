<Window x:Class="QuanLyKhachSan.View.Windows.ListDetailReport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Chi tiết báo cáo doanh thu" Height="300" Width="500" FontFamily="Times New Roman" FontSize="15">
    <Grid>
        <ListView Name="listDetailReport" >
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Tên Loại Phòng" DisplayMemberBinding="{Binding nameTypeRoom}"/>
                    <GridViewColumn Header="Doanh thu" DisplayMemberBinding="{Binding totalMoney}"/>
                    <GridViewColumn Header="Tỷ lệ" DisplayMemberBinding="{Binding percent}"/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
