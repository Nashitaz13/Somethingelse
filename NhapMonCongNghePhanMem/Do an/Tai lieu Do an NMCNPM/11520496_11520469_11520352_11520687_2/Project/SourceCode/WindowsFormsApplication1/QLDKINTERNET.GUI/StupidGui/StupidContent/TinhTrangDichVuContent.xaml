        <DataGrid Name="dgDichVuMo"  HorizontalAlignment="Left" Height="122" Margin="10,232,-490,-54" VerticalAlignment="Top" Width="780" SelectionChanged="dgDichVuMo_SelectionChanged"/>
        <Button Name="btnMo"  Content="Mở" HorizontalAlignment="Left" Height="24" Margin="321,171,-189,0" VerticalAlignment="Top" Width="168" Click="btnMo_Click"/>
        <Button Name="btnTat" Content="Tắt" HorizontalAlignment="Left" Height="24" Margin="321,359,-189,-83" VerticalAlignment="Top" Width="168" Click="btnTat_Click"/>

    </Grid>
</UserControl>
